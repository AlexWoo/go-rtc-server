if [ $# -ne 1 ];then
    echo "Usage: $1 plugin_name"
    exit 1
fi

CURDIR=`pwd`
OLDGOPATH="$GOPATH"
export GOPATH="$CURDIR"

echo $GOPATH

rtcpath=/usr/local/gortc
pluginpath=./

cd src/$1
go get

echo "go build -buildmode=plugin -o $rtcpath/slp/$1.so"
go build -buildmode=plugin -o $rtcpath/slp/$1.so

export GOPATH="$OLDGOPATH"
