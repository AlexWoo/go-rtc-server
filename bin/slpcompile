if [ $# -ne 1 ];then
    echo "Usage: $1 plugin_name"
    exit 1
fi

rtcpath=/usr/local/gortc

CURDIR=`pwd`
OLDGOPATH="$GOPATH"
export GOPATH="$CURDIR"

echo "slpcompile $rtcpath/slp/$1.so $1/$1.go"
go build -ldflags "-pluginpath=$1" -buildmode=plugin -o $rtcpath/slp/$1.so $1/$1.go

export GOPATH="$OLDGOPATH"
