#!/bin/bash

# Copyright (C) AlexWoo(Wu Jie) wj19840501@gmail.com


. auto/options

case ".$RTCPATH" in
    .)  RTCPATH=/usr/local/gortc    ;;
    .!) RTCPATH=./                  ;;
    *)                              ;;
esac

MACROFILE=src/gortc/macro.go

. auto/tmpl

key=RTCPATH
value="\"$RTCPATH\""
. auto/macro

DEPENDENCE=`cat auto/dependence`

. auto/install
