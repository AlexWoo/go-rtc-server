#!/bin/bash

# Copyright (C) AlexWoo(Wu Jie) wj19840501@gmail.com


. auto/options

case ".$RTCPATH" in
    .)  RTCPATH=/usr/local/gortc/   ;;
    .!) RTCPATH=./                  ;;
    *)                              ;;
esac

sed -i "s#rtcpath = \".*\"#rtcpath = \"$RTCPATH\"#" src/gortc/gortc.go

DEPENDENCE=`cat auto/dependence`

. auto/install
